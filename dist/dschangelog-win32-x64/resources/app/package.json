{
  "name": "dschangelog",
  "version": "0.1.0",
  "description": "Changelog manager",
  "main": "./main/main.js",
  "scripts": {
    "start": "electron ./main/main.js",
    "test": "mocha --timeout 2000",
    "build-linux": "electron-packager . --electron-version 1.4.15 --platform linux --icon ../assets/icon.png --out ./dist --overwrite",
    "build-win32": "electron-packager . --electron-version 1.4.15 --platform win32 --icon ../assets/icon.ico --out ./dist --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danieluy/ds_changelog.git"
  },
  "author": "http://www.danielsosa.uy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danieluy/ds_changelog/issues"
  },
  "homepage": "https://github.com/danieluy/ds_changelog#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "^1.4.15",
    "electron-packager": "^8.5.2"
  }
}